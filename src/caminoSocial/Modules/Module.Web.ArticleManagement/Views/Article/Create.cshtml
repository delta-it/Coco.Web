@model ArticleModel
@{
    ViewData["Title"] = "Create article";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts {
    <script src="~/lib/tinymce/tinymce.min.js" asp-append-version="true"></script>
    <script src="~/articlemanagement/js/article-update.js" asp-append-version="true"></script>
}

<div class="card-header bg-dark text-light">
    <a asp-action="Index" asp-controller="Article" class="btn btn-sm btn-secondary">
        <i class="fa fa-arrow-left"></i>
    </a>
    @ViewData["Title"]
</div>
<div class="card-body">
    <form method="post" asp-action="Create" asp-controller="Article">
        <partial name="_CreateOrUpdateForm" />
    </form>
</div>
